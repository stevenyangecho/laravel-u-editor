{
  "name": "stevenyangecho/laravel-u-editor",
  "description": "UEditor for laravel5. Support i18n. UEditor is a Rich Text Web Editor From Baidu.",
  "keywords": [
    "laravel",
    "ueditor",
    "laravel-u-editor",
    "laravel5 web text editor",
    "stevenyangecho",
    "steven yang"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Steven Yang",
      "email": "yqmking@163.com"
    }
  ],

  "require": {
    "php": ">=5.4.0",
    "ext-fileinfo": "*",
    "illuminate/support": "5.*",
    "qiniu/php-sdk": "7.*"
  },
  "autoload": {
    "psr-4": {
      "Stevenyangecho\\UEditor\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
      "laravel": {
          "providers": [
              "Stevenyangecho\\UEditor\\UEditorServiceProvider"
          ],
          "aliases": {
              "UEditor": "Stevenyangecho\\UEditor\\UEditorServiceProvider"
          }
      }
  }  
}
